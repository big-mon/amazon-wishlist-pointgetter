{
  "name": "amazon-wishlist-pointgetter",
  "description": "Google Chrome Extension for Amazon.co.jp",
  "version": "1.5.3",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/big-mon/amazon-wishlist-pointgetter",
  "author": "BIG-MON <> (https://github.com/big-mon)",
  "scripts": {
    "build": "pnpm run sync-version && webpack --mode=production",
    "dev": "pnpm run sync-version && webpack --mode=development",
    "watch": "webpack --watch --mode=development",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "deploy-chrome": "pnpm run sync-version && node scripts/deploy.js",
    "zip": "cd dist && zip -r ../extension.zip .",
    "sync-version": "node scripts/sync-version.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {}
}
